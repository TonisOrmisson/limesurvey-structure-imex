{
    "name": "tonisormisson/limesurvey-relevance-imex",
    "description": "A LimeSurvey Plugin to allow Import & Export of survey Relevance equations",
    "keywords": ["limesurvey"],
    "homepage": "https://github.com/TonisOrmisson/limesurvey-relevance-imex",
    "type": "limesurvey-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "TÃµnis Ormisson",
            "email": "tonis@andmemasin.eu",
            "homepage": "https://andmemasin.eu",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
      "php": ">=8.3.0 <8.4",
        "ext-json": "*",
        "openspout/openspout": "^4.0",
        "sebastian/version": "^6"
    },
    "require-dev": {
        "limesurvey/limesurvey": "^6",
        "phpstan/phpstan": "^1|^2",
        "phpunit/phpunit": "^9|^10|^11|^12"
    },
    "config": {
        "platform": {
          "php": "8.3.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "tonisormisson\\ls\\structureimex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tonisormisson\\ls\\structureimex\\Tests\\Unit\\": "tests/unit",
            "tonisormisson\\ls\\structureimex\\Tests\\Functional\\": "tests/functional"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-unit": "phpunit --testsuite=unit",
        "test-functional": "phpunit --testsuite=functional",
        "test-coverage": "phpunit --coverage-html coverage",
        "test-setup": "bin/setup-test-db.sh",
        "phpstan": "phpstan analyse src"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
